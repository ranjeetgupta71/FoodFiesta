
<h2>Sign In</h2>

<%= form_with(model: @user, url: '/signin', method: 'post') do |form| %>
  <div>
    <%= form.label :email %>
    <%= form.email_field :email %>
  </div>

  <div>
    <%= form.label :password %>
    <%= form.password_field :password %>
  </div>

  <div>
    <%= form.submit 'Sign In' %>
  </div>
<% end %>
